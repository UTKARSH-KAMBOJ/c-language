#include<stdio.h>
 int main(){
   int a;
   int b;
   int n;
   printf("enter the operation which you want to perform 1.Relational\n 2. logical \n 3. bitwise \n 4. assignment \n 5. increment decrement operators ");
   scanf("%d",&n);
   printf("enter thr value of a:");
   scanf("%d",&a);
   printf("enter the value of b:");
   scanf("%d",&b);
   int m;
   int i;
   switch(n){
      case 1:
      
      printf("enter 1. > \n 2. < \n 3. <= \n4. >= \n5. ==\n 6.!=");
      scanf("%d",&m);
      switch(m){
         case 1:
         if(a>b){
            printf("a is greater then b");
         }
         else {
            printf("b is greater then a");
            break;
         }
         case 2:
         if(a<b){
            printf("b is greater than a");

         }
         else{
            printf("a is greater then b");
         }
         case 3:
         if(a<=b){
            printf("a is less than equal to b");
         }
         else {
            printf("b is less than equal to a");
         }
         case 4:
         if(a>=b){
            printf("a is greater then equal to b");
         }
         else{
            printf("b is grater then equal to a");
         }
         case 5:
         if(a==b){
            printf("a is equal to b");
         }
         else {
            printf("a is not equal to b");
         }
         case 6:
         if(a!=b){
            printf("a is not equal to b");
         }
         else{
            printf("a is equal to b");
         }
      }
      case 2:
    
      printf("enter 1.&& \n 2. ||\n 3. !");
      scanf("%d",&i);
      switch(i){
         case 1:
         if(a==0&&b==0){
            printf("a and b both are zero");
         }
         else{
            printf("a or b not equal to zero");
         }
         case 2:
         if(a==0||b==0){
            printf("either a is equal to zero or b will be equal to zero");
         }
         case 3:
         if(a!=b){
            printf("a is not equal to b");

         }
         else {
            printf("a is equal to b");
         }
      }
      
   }
   return 0;
 }
