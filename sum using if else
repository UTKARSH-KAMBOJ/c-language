int main(){
    int n,m;
    printf("enter the value of n:");
    scanf("%d",&n);
    printf("enter the value of m:");
    scanf("%d",&m);
    int i;
    int sum =0;
    int sum1=0;
    int a=0;
    for(i=1;i<=n;i++){
        if(i%m != 0){
            sum+=i;
            
        }
        else {
            sum1+=i;
        }
    }
    a=sum-sum1;
    printf("%d\n",sum1);
    printf("%d\n",sum);


    printf("%d",a);
    return 0;
    

}
