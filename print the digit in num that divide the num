#include <stdio.h>

int main() {
    int num = 0;
    printf("Enter the number of your choice: ");
    scanf("%d", &num);

    int sum = 0;
    int originalVal = num;

    while (num > 0) {
        int val = num % 10;
        if (val != 0 && originalVal % val == 0) {
            sum++;
        }
        num = num / 10;
    }

    printf("Number of digits that divide %d: %d\n", originalVal, sum);
    return 0;
}
