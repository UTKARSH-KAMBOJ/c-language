#include<stdio.h>
int main(){
    int n;
    printf("enter the value of n:");
    scanf("%d",&n);
    int i,j;
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
            if(i==n||j==1||j==n){
                printf("*");}
                else{
                    printf(" ");
                }
            
            
        }
        printf("\n");
    }
    return 0;
}
